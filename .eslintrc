{
  "env": {
    "es2021": true,
    "node": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 1,
    "prettier/prettier": 2,
    "no-unused-vars": 2,
    "no-unused-expressions": 2
  }
}
